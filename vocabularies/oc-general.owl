        <rdfs:subClassOf rdf:resource="&oc-general;predicates"/>
        <rdfs:comment rdf:datatype="&xsd;string">A type of predicate that is used to provide description to an item, rather than assert some sort of linking relationship to other items. </rdfs:comment>
    </owl:Class>
    


