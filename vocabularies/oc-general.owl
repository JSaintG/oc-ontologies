     xmlns:oc-general="http://opencontext.org/vocabularies/oc-general/"
     xmlns:cidoc-crm="http://erlangen-crm.org/current/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:crm-eh="http://purl.org/crmeh#">
    <owl:Ontology rdf:about="http://opencontext.org/vocabularies/oc-general">
        <rdfs:label>Open Context Concepts</rdfs:label>
        <rdfs:comment>General concepts applied to all data published by Open Context</rdfs:comment>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&oc-general;location-precision-note">
        <rdfs:label xml:lang="en">Location precision note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property provides a verbal description or comment about the precision of the geospatial metadata provided for an item.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdfs;comment"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&oc-general;reference-label">
        <rdfs:label xml:lang="en">Reference label</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property labels an item that has geospatial or chronological metadata specified with the reference-uri property.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdfs;label"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&oc-general;location-note">
        <rdfs:label xml:lang="en">Location note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property a verbal description of a geospatial feature associated with an item.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdfs;comment"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&oc-general;hasIcon">
        <rdfs:label xml:lang="en">Has Icon</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&foaf;depiction"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&skos;closeMatch"/>
    <owl:AnnotationProperty rdf:about="&oc-general;obsNote">
        <rdfs:label xml:lang="en">Observation note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property a verbal description / comment about an observation event.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdfs;comment"/>
    </owl:AnnotationProperty>
    <owl:AnnotationProperty rdf:about="&foaf;depiction"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://opencontext.org/vocabularies/oc-general/category -->

    <owl:ObjectProperty rdf:about="&oc-general;category">
        <rdfs:label xml:lang="en">Category</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This is basically just the same as rdf:type. It&apos;s defined here as a bit of a hack to avoid JSON-LD validation problems AND making sure that the JSON-LD still works as GeoJSON (software consuming GeoJSON does not like lists of multiple &apos;types&apos;).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/contains -->

    <owl:ObjectProperty rdf:about="&oc-general;contains">
        <rdfs:label xml:lang="en">Contains</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property describes very general spatial or contextual &quot;containment&quot; relationships that describe how entities published by Open Context relate. In the sense defined here, containment relationships help organize entities into contextual relationships as they were observed during the process of discovery and description.</rdfs:comment>
        <rdfs:domain rdf:resource="&oc-general;contents"/>
        <rdfs:range rdf:resource="&oc-general;subjects"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/has-contents -->

    <owl:ObjectProperty rdf:about="&oc-general;has-contents">
        <rdfs:label xml:lang="en">Has contents</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property indicates that an item contains child items in a containment hierarchy.</rdfs:comment>
        <rdfs:range rdf:resource="&oc-general;contents"/>
        <rdfs:domain rdf:resource="&oc-general;subjects"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/has-context-path -->

    <owl:ObjectProperty rdf:about="&oc-general;has-context-path">
        <rdfs:label xml:lang="en">Has context path</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property indicates that an item has items that contain it in a containment hierarchy.</rdfs:comment>
        <rdfs:range rdf:resource="&oc-general;contexts"/>
        <rdfs:domain rdf:resource="&oc-general;subjects"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/has-linked-context-path -->

    <owl:ObjectProperty rdf:about="&oc-general;has-linked-context-path">
        <rdfs:label xml:lang="en">Has linked context path</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property indicates that an item is linked to a subjects item with a containment hierarchy.</rdfs:comment>
        <rdfs:range rdf:resource="&oc-general;contexts"/>
        <rdfs:domain rdf:resource="&oc-general;items"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/has-obs -->

    <owl:ObjectProperty rdf:about="&oc-general;has-obs">
        <rdfs:label xml:lang="en">Has observation</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property indicates that an item has an associated observation event.</rdfs:comment>
        <rdfs:domain rdf:resource="&oc-general;items"/>
        <rdfs:range rdf:resource="&oc-general;observations"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/has-path-items -->

    <owl:ObjectProperty rdf:about="&oc-general;has-path-items">
        <rdfs:label xml:lang="en">Has context path items</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property identifies members of a containment hierarchy.</rdfs:comment>
        <rdfs:domain rdf:resource="&oc-general;contexts"/>
        <rdfs:range rdf:resource="&oc-general;subjects"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/location-note -->

    <owl:ObjectProperty rdf:about="&oc-general;location-note">
        <rdfs:label xml:lang="en">Location note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property a verbal description of a geospatial feature associated with an item.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/location-precision -->

    <owl:ObjectProperty rdf:about="&oc-general;location-precision">
        <rdfs:label xml:lang="en">Location precision</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property describes the precision of the geospatial metadata provided for an item as a &apos;zoom level&apos;. A zero means that the precision was not intentially reduced. A positive integer means an intentional reduction in precision to a given zoom level. Reduced precision means that the geospatial information has sensitivity / security issues.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/location-precision-note -->

    <owl:ObjectProperty rdf:about="&oc-general;location-precision-note">
        <rdfs:label xml:lang="en">Location precision note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property provides a verbal description or comment about the precision of the geospatial metadata provided for an item.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/obsNote -->

    <owl:ObjectProperty rdf:about="&oc-general;obsNote">
        <rdfs:label xml:lang="en">Observation note</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property a verbal description / comment about an observation event.</rdfs:comment>
        <rdfs:domain rdf:resource="&oc-general;observations"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/obsStatus -->

    <owl:ObjectProperty rdf:about="&oc-general;obsStatus">
        <rdfs:label xml:lang="en">Observation status</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property indicates if an observation is still &apos;active&apos; and valid, or if it is &apos;depricated&apos; and no longer valid.</rdfs:comment>
        <rdfs:domain rdf:resource="&oc-general;observations"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/predType -->

    <owl:ObjectProperty rdf:about="&oc-general;predType">
        <rdfs:label xml:lang="en">Predicate type</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Indicates the type of predicate (a &apos;variable&apos; for descriptive properties) or a &apos;link&apos; to other Open Context media, subjects, persons, projects, or documents items.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&rdf;type"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/reference-label -->

    <owl:ObjectProperty rdf:about="&oc-general;reference-label">
        <rdfs:label xml:lang="en">Reference label</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property labels an item that has geospatial or chronological metadata specified with the reference-uri property.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/reference-type -->

    <owl:ObjectProperty rdf:about="&oc-general;reference-type">
        <rdfs:label xml:lang="en">Reference type</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property describes the source of geospatial or chronological metadata. If the source of the metadata was the item itself, the object of this property is &apos;specified&apos;. If the source of the metadata was some other item, the object of this property is &apos;inferred&apos;, since the geospatial or chronological information was inferred through containment relationships.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/reference-uri -->

    <owl:ObjectProperty rdf:about="&oc-general;reference-uri">
        <rdfs:label xml:lang="en">Reference URI</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">This property identifies an item that has geospatial or chronological metadata specified. This is useful, since many items have geospatial and chronological metadata not directly assigned, but provided through inference.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/slug -->

    <owl:ObjectProperty rdf:about="&oc-general;slug">
        <rdfs:label xml:lang="en">Slug</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">A secondary identifier for an item, used to make more human-readable URLs, so it&apos;s easier to understand and debug requests made with Web services / APIs.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    </owl:ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/oc-general/sourceID -->

    <owl:ObjectProperty rdf:about="&oc-general;sourceID">
        <rdfs:label xml:lang="en">Source ID</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Identifies the data source (usually an imported data table processed in creating a record in Open Context). This is mainly indicated for Open Context&apos;s internal administrative and management purposes, and likely does not have much use for outside users of data published by Open Context.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/identifier"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/terms/identifier -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/identifier"/>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#Property -->

    <owl:ObjectProperty rdf:about="&rdf;Property"/>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#type -->

    <owl:ObjectProperty rdf:about="&rdf;type"/>
    


