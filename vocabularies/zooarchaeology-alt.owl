    <!-- http://opencontext.org/vocabularies/open-context-zooarch/zoo-0077 -->

    <ObjectProperty rdf:about="http://opencontext.org/vocabularies/open-context-zooarch/zoo-0077">
        <rdfs:label rdf:datatype="&rdfs;Literal">Has fusion character</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Has bone fusion characterization. Describes a bone fusion stage.</rdfs:comment>
        <rdfs:range rdf:resource="http://opencontext.org/vocabularies/open-context-zooarch/zoo-0061"/>
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P2_has_type"/>
    </ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/open-context-zooarch/zoo-0078 -->

    <ObjectProperty rdf:about="http://opencontext.org/vocabularies/open-context-zooarch/zoo-0078">
        <rdfs:label rdf:datatype="&rdfs;Literal">Has physiological sex determination</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Has an identication, with some degree of certainty, as to the physiological sex of an organism.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P2_has_type"/>
    </ObjectProperty>
    


    <!-- http://opencontext.org/vocabularies/open-context-zooarch/zoo-0079 -->

    <ObjectProperty rdf:about="http://opencontext.org/vocabularies/open-context-zooarch/zoo-0079">
        <rdfs:label rdf:datatype="&rdfs;Literal">Has anatomical identification</rdfs:label>
        <rdfs:comment rdf:datatype="&xsd;string">Has an identification (with some degree of certainty and confidence) to a specific anatomical structure. For zooarchaeology, this usually means an identification to a bone element</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/UBERON_0001062"/>
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P2_has_type"/>
    </ObjectProperty>
    


    <!-- http://erlangen-crm.org/current/P2_has_type -->

